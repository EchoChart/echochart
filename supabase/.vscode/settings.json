{
   "files.exclude": {
      "node_modules/": true,
      "**/.DS_Store": true
   },
   "eslint.workingDirectories": [{ "mode": "auto" }, { "directory": "." }],
   "editor.formatOnSave": true,
   "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "always",
      "source.organizeImports": "always"
   },
   "eslint.validate": ["javascript", "typescript"],
   "[javascript][json][typescript][yaml]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
   },
   "[sql]": {
      "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
   },
   "deno.enable": false,
   "deno.lint": true,
   "deno.unstable": [
      "bare-node-builtins",
      "byonm",
      "sloppy-imports",
      "unsafe-proto",
      "webgpu",
      "broadcast-channel",
      "worker-options",
      "cron",
      "kv",
      "ffi",
      "fs",
      "http",
      "net"
   ],
   "deno.enablePaths": ["./functions"],
   "deno.importMap": "./functions/import_map.json"
}
