{
   "root": true,
   "env": {
      "node": true
   },
   "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
   "parser": "@typescript-eslint/parser",
   "parserOptions": {
      "ecmaVersion": "latest"
   },
   "plugins": ["@typescript-eslint"],
   "rules": {
      "@typescript-eslint/no-unused-vars": "warn",
      "@typescript-eslint/no-explicit-any": "warn",
      "no-console": "off"
   }
}
