{
   "name": "echochart-app",
   "version": "1.5.2-alpha.1",
   "type": "module",
   "scripts": {
      "dev": "pnpm dev:supabase-types && pnpm dev:vue",
      "dev:vue": "vite --host",
      "dev:supabase": "supabase start --ignore-health-check",
      "dev:supabase-types": "supabase gen types typescript --local > src/@types/database.types.ts",
      "dev:supabase-reset": "supabase db reset && pnpm dev:supabase-types",
      "build": "vite build",
      "preview": "vite preview",
      "lint": "eslint --fix --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore",
      "commit": "cz",
      "i18n-extract": "vue-i18n-extract report",
      "i18n-build": "node src/script/translate-with-ollama.js"
   },
   "dependencies": {
      "@casl/ability": "^6.7.3",
      "@casl/vue": "^2.2.2",
      "@primeuix/themes": "^1.0.0",
      "@primevue/core": "^4.2.5",
      "@supabase/supabase-js": "^2.47.10",
      "@tanstack/query-persist-client-core": "^5.76.1",
      "@tanstack/query-sync-storage-persister": "^5.76.1",
      "@tanstack/vue-query": "^5.76.0",
      "@vueuse/core": "^11.3.0",
      "axios": "^1.12.0",
      "chart.js": "3.3.2",
      "countries-list": "^3.1.1",
      "dayjs": "^1.11.13",
      "deep-object-diff": "^1.1.9",
      "idb-keyval": "^6.2.2",
      "jwt-decode": "^4.0.0",
      "libphonenumber-js": "^1.12.6",
      "lodash": "^4.17.21",
      "lz-string": "^1.5.0",
      "mitt": "^3.0.1",
      "pinia": "^2.3.0",
      "primeicons": "^7.0.0",
      "primelocale": "^2.1.7",
      "primevue": "^4.3.3",
      "quill": "^2.0.3",
      "validatorjs": "^3.22.1",
      "vue": "^3.5.16",
      "vue-i18n": "^10.0.8",
      "vue-router": "^4.5.0"
   },
   "devDependencies": {
      "@primevue/auto-import-resolver": "^4.2.5",
      "@rushstack/eslint-patch": "^1.10.4",
      "@types/lodash": "^4.17.14",
      "@types/node": "^22.10.5",
      "@types/validatorjs": "^3.15.5",
      "@typescript-eslint/eslint-plugin": "^8.33.0",
      "@typescript-eslint/parser": "^8.33.0",
      "@vitejs/plugin-vue": "^5.2.4",
      "@vue/eslint-config-prettier": "^9.0.0",
      "@vue/eslint-config-typescript": "^14.5.1",
      "autoprefixer": "^10.4.20",
      "commitizen": "^4.3.1",
      "cz-conventional-changelog": "^3.3.0",
      "eslint": "^8.57.1",
      "eslint-plugin-vue": "^9.32.0",
      "husky": "^9.1.7",
      "lint-staged": "^15.3.0",
      "postcss": "^8.4.49",
      "prettier": "^3.5.3",
      "release-please": "^16.15.0",
      "sass": "^1.83.1",
      "tailwindcss": "^3.4.17",
      "tailwindcss-primeui": "^0.3.4",
      "typescript": "^5.7.2",
      "unplugin-auto-import": "^0.18.6",
      "unplugin-vue-components": "^0.27.5",
      "vite": "^5.4.21",
      "vite-plugin-commonjs": "^0.10.4",
      "vite-plugin-compression": "^0.5.1",
      "vite-plugin-package-version": "^1.1.0",
      "vue-i18n-extract": "^2.0.7"
   },
   "config": {
      "commitizen": {
         "path": "cz-conventional-changelog"
      }
   },
   "lint-staged": {
      "*.{js,vue,ts,json}": "pnpm lint"
   }
